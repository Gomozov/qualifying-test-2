<tr class="<%= if @lib.commited > 182 do %>old<% end %>">
  <td><b><%= link @lib.name, to: @lib.url %></b></td>
  <td><%= raw(@lib.desc) %></td>
  <td nowrap>
    <%= if @lib.stars do %>
      ⭐<%= @lib.stars %> 
    <% end %>
  </td>
  <td nowrap>
    <%= if @lib.commited do %>
      🗓<%= @lib.commited %> 
    <% end %>
  </td>
</tr>
